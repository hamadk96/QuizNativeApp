<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gebruiker.quiznativeapp.feature.Highscore">

    <include
        android:id="@+id/actionbar"
        layout="@layout/actionbar_mainactivity" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/actionbar">


    </RelativeLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="Highscores"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.156"
        tools:layout_editor_absoluteX="8dp" />

    <TextView
        android:id="@+id/lblPerCategorie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Per categorie"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.424"
        tools:layout_editor_absoluteX="151dp" />

    <TextView
        android:id="@+id/lblPerType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Per type"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="126dp" />

    <Button
        android:id="@+id/btnTop10"
        android:layout_width=""
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:text="Top 10"
        app:layout_constraintEnd_toStartOf="@+id/btnTop100"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="167dp" />

    <Button
        android:id="@+id/btnTop100"
        android:layout_width= ""
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:text="Top 100"
        app:layout_constraintEnd_toStartOf="@+id/btnMijnTop10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnTop10"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="167dp" />

    <Button
        android:id="@+id/btnMijnTop10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mijn top 10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnTop100"
        tools:layout_editor_absoluteY="167dp" />

    <Button
        android:id="@+id/btnAlScores"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="52dp"
        android:text="Al mijn scores"
        app:layout_constraintEnd_toStartOf="@+id/btnMeestRecent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="228dp" />

    <Button
        android:id="@+id/btnMeestRecent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meest recente"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnAlScores"
        tools:layout_editor_absoluteY="228dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="340dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="399dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="340dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="399dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="470dp" />

</android.support.constraint.ConstraintLayout>
